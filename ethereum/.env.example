# Deployment Configuration
# Copy this file to .env and fill in the values

# ============ REQUIRED ============

# Your wallet private key (without 0x prefix)
# WARNING: Never commit this file with real keys!
PRIVATE_KEY=your_private_key_here

# ============ OPTIONAL ============

# RPC URLs (defaults provided)
SEPOLIA_RPC_URL=https://rpc.sepolia.org
MAINNET_RPC_URL=https://eth.llamarpc.com

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=

# Custom USDT address (leave empty to deploy MockUSDT on testnet)
USDT_ADDRESS=

# Custom owner address (leave empty to use deployer)
OWNER_ADDRESS=

# Multi-sig signers (comma-separated addresses)
# Leave empty to use deployer as both signers for testing
MULTISIG_SIGNERS=

# Required signatures for multi-sig (default: 2)
REQUIRED_SIGS=2

# ============ MAINNET ONLY ============
# These are required for mainnet deployment

# ZK Verifier contract address
ZK_VERIFIER_ADDRESS=

# Ring Signature Verifier contract address
RING_VERIFIER_ADDRESS=
